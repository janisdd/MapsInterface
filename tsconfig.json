{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "files": [
        "google/google.maps.d.ts",
        "google/googleMapsImplementation.ts",
        "googleClusterTest.ts",
        "mapClusterer.ts",
        "mapInterface.ts",
        "mapSelectionHelper.ts",
        "old/mapClusterer.ts"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
